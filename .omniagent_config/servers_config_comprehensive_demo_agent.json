{
  "AgentConfig": {
    "agent_name": "comprehensive_demo_agent",
    "tool_call_timeout": 60,
    "max_steps": 15,
    "request_limit": 0,
    "total_tokens_limit": 0,
    "enable_tools_knowledge_base": false,
    "tools_results_limit": 10,
    "tools_similarity_threshold": 0.1,
    "memory_config": {
      "mode": "sliding_window",
      "value": 100
    },
    "memory_results_limit": 5,
    "memory_similarity_threshold": 0.5,
    "memory_tool_backend": null
  },
  "LLM": {
    "provider": "mistral",
    "model": "magistral-medium-2509",
    "temperature": 0.3,
    "max_tokens": 5000,
    "max_context_length": 1000,
    "top_p": 0.7,
    "top_k": "N/A"
  },
  "mcpServers": {
    "filesystem": {
      "transport_type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/abiorh/Desktop",
        "/home/abiorh/ai/"
      ]
    }
  },
  "EMBEDDING": {
    "provider": "voyage",
    "model": "voyage-3.5",
    "dimensions": 1024,
    "encoding_format": "base64",
    "timeout": 600
  }
}